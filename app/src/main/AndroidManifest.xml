<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ester.calm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".view.MainActivity"></activity>
        <activity
            android:name=".view.NotificationActivity"
            android:label="Notifikasi" />
        <activity android:name=".view.UserOrderActivity">

        </activity>
        <activity
            android:name=".view.Splashscreen"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity android:name=".view.LoginActivity"></activity>
        <activity android:name=".view.OrderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.OrderDetailActivity"></activity>
        <activity android:name=".view.ConfirmActivity" />
        <activity android:name=".view.RequestActivity" />
        <activity android:name=".view.ProdukDetailActivity" />
        <activity
            android:name=".view.CustomViewIconTextTabsActivity"
            android:label="@string/title_activity_custom_view_icon_text_tabs"></activity>
        <activity android:name=".view.UserOrderDiterima"></activity>
    </application>

</manifest>